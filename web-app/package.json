{
  "name": "memeory",
  "version": "1.0.0",
  "private": false,
  "homepage": "https://sokomishalov.github.io/memeory",
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@fortawesome/fontawesome-svg-core": "~1.2.30",
    "@fortawesome/free-brands-svg-icons": "~5.14.0",
    "@fortawesome/free-solid-svg-icons": "~5.14.0",
    "@fortawesome/react-fontawesome": "~0.1.11",
    "antd": "^4.6.3",
    "antd-mobile": "^2.3.3",
    "axios": "^0.20.0",
    "copy-to-clipboard": "~3.3.1",
    "detect-browser-language": "0.0.2",
    "i18next": "~19.7.0",
    "i18next-browser-languagedetector": "~6.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-aspect-ratio": "~1.0.42",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "~11.7.2",
    "react-infinite-scroller": "~1.2.4",
    "react-player": "^2.6.2",
    "react-redux": "~7.2.1",
    "react-reveal": "^1.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "~4.0.5"
  },
  "devDependencies": {
    "babel-plugin-import": "~1.13.0",
    "customize-cra": "~1.0.0",
    "gh-pages": "^3.1.0",
    "less": "^3.12.2",
    "node-sass-chokidar": "1.5.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "~15.7.2",
    "react-app-rewired": "~2.1.6",
    "react-scripts": "~5.0.1",
    "source-map-explorer": "~2.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build-less": "lessc --js src/app/common/styles/antd.less src/app/common/styles/antd.css",
    "watch-css": "npm run build-less && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "npm run build-less && node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-app-rewired build",
    "predeploy": "npm run build && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
